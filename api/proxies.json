{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "theAPI": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/api/{*restOfPath}"
      },
      "backendUri": "http://localhost:7071/api/{restOfPath}"
    },
    "root": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/{*restOfPath}"
      },
      "backendUri": "http://localhost:7071/api/public?file={restOfPath}"
    },
    "root1": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/"
      },
      "backendUri": "http://localhost:7071/api/public?file=index.html"
    }
  }
}